<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="futsal.mybatis">

	<select id="selectUser" resultType="map" parameterType="com.wicket_futsal.dto.UserDTO">
    SELECT
      PASSWORD
    FROM
      M_USER
    WHERE
      USER_ID = #{userId};
	</select>
</mapper>